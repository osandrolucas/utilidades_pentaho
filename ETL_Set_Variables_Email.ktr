        <class_source>String environmentField;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] r = getRow();

	if (r == null) {
		setOutputDone();
		return false;
	}

	environmentField = getParameter("ENVIRONMENT_FIELD");

	String environment = System.getenv("ENV") != null ? System.getenv("ENV") : "dev";

    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());

	get(Fields.Out, environmentField).setValue(outputRow, environment);

    putRow(data.outputRowMeta, outputRow);

	return true;
}</class_source>
